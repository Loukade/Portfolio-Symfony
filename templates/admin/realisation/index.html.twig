{% extends 'base.html.twig.' %}
{% block title 'Gérer les Presentations' %}
{% block body %}

    <div></div>
    <h1 class="black">Gérer les Réalisations</h1><br><br><br>
    <div class="block">
        <fieldset>
            <table>
                <thead>
                <tr>
                    <th>Titre</th>

                </tr>

                </thead>

                <tbody>
                {% for realisation in realisations %}
                    <tr>
                        <td>{{ realisation.titre }}</td>

                        <td>
                            <a href="{{ path('admin.realisations.edit' , {id : realisation.id}) }}" class ="btn btn-secondary ">Editer </a>
                        </td>
                    </tr>

                {% endfor %}
            </table>
        </fieldset>
        </tbody>
    </div>
    <br><br><br><br><br><br><br><br><br><br><br>
    {% include ("pages/includes/footer.html.twig") %}




{% endblock  %}